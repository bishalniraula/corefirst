@model CoreFirst.Models.Student
@{
    ViewData["Title"] = "Student";
}
<div class="text-center">
    <h1 class="display-4">student information</h1>
    <div class="alert alert-dark" role="alert">
       data updatd successfully
    </div>
    @{
        if (@Model?.Id==null)
        {
            <p>No data Found</p>
        }
        else
        {
            <p>Id:@Model.Id</p>
            <p>Name:@Model.Name</p>
            <p>Gender:@Model.Gender</p>
        }
    }
   

</div>
